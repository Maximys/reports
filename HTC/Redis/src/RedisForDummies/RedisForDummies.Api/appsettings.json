{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApacheIgniteSettings": {
    "Endpoints": [
      "ignite_instance:10800"
    ]
  },
  "RedisSettings": {
    //По-умолчанию контейнер Redis'а запускается на порту 6379.
    "Configuration": "redis_instance:6379",
    "InstanceName": "MySuperCoolSampleInstance"
  }
}
